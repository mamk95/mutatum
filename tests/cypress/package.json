{
  "name": "mutatum-cypress-tests",
  "version": "1.0.0",
  "description": "Cypress tests for the Mutatum project",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:open:mobile": "cypress open --config viewportWidth=375,viewportHeight=667",
    "cypress:run:firefox": "cypress run --browser firefox --spec 'cypress/tests/ui/*'",
    "cypress:run:chrome": "cypress run --browser chrome --spec 'cypress/tests/ui/*'",
    "cypress:run:mobile:firefox": "cypress run --browser firefox --spec 'cypress/tests/ui/*' --config viewportWidth=375,viewportHeight=667",
    "cypress:run:mobile:chrome": "cypress run --browser chrome --spec 'cypress/tests/ui/*' --config viewportWidth=375,viewportHeight=667"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mamk95/mutatum.git"
  },
  "author": "Magnus Karlsen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mamk95/mutatum/issues"
  },
  "homepage": "https://github.com/mamk95/mutatum#readme",
  "devDependencies": {
    "cypress": "^9.1.1"
  }
}
