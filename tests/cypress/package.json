{
  "name": "mutatum-cypress-tests",
  "version": "1.0.0",
  "description": "Cypress tests for the Mutatum project",
  "main": "index.js",
  "scripts": {
    "cypress:open:desktop": "cypress open --config viewportWidth=1920,viewportHeight=1080",
    "cypress:open:mobile": "cypress open --config viewportWidth=375,viewportHeight=667",
    "cypress:run:api": "cypress run --browser firefox --spec 'cypress/tests/api/*'",
    "cypress:run:ui:desktop:firefox": "cypress run --browser firefox --spec 'cypress/tests/ui/*' --config viewportWidth=1920,viewportHeight=1080",
    "cypress:run:ui:desktop:chrome": "cypress run --browser chrome --spec 'cypress/tests/ui/*' --config viewportWidth=1920,viewportHeight=1080",
    "cypress:run:ui:mobile:firefox": "cypress run --browser firefox --spec 'cypress/tests/ui/*' --config viewportWidth=375,viewportHeight=667",
    "cypress:run:ui:mobile:chrome": "cypress run --browser chrome --spec 'cypress/tests/ui/*' --config viewportWidth=375,viewportHeight=667"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mamk95/mutatum.git"
  },
  "author": "Magnus Karlsen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mamk95/mutatum/issues"
  },
  "homepage": "https://github.com/mamk95/mutatum#readme",
  "devDependencies": {
    "cypress": "^9.4.1"
  }
}
